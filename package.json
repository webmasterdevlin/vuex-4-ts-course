{
  "name": "vuex-4-ts-course",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend": "json-server --watch db.json --port 5000 --delay=1000",
    "serve:fullstack": "concurrently \"npm run backend\" \"npm run serve\"",
    "test": "npx cypress open",
    "test:open": "npx cypress open --browser edge",
    "test:cypress-headed": "npx cypress --headed",
    "tsc:check": "npm run tsc:src && npm run tsc:cypress",
    "tsc:src": "tsc --noEmit --project tsconfig.json",
    "tsc:cypress": "tsc --noEmit --project cypress/tsconfig.json",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "core-js": "^3.16.1",
    "vee-validate": "^4.4.9",
    "vue": "^3.2.1",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^1.5.0",
    "@cypress/code-coverage": "^3.9.10",
    "@testing-library/cypress": "^7.0.7",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.13",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.2.1",
    "concurrently": "^6.2.1",
    "cypress": "^6.9.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-testing-library": "^3.10.2",
    "istanbul-lib-coverage": "^3.0.0",
    "json-server": "^0.16.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "typescript": "~4.3.5",
    "uuid": "^8.3.2"
  }
}
